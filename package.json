{
  "name": "di-js",
  "version": "1.0.0",
  "description": "Dependency injection library for Javascript",
  "main": "lib/di.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "atma test",
    "build": "app-bundler"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tenbits/di.git"
  },
  "keywords": [
    "di",
    "dependency",
    "injection",
    "IoC"
  ],
  "author": "Alex Kit (alex.kit@atmajs.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tenbits/di/issues"
  },
  "homepage": "https://github.com/tenbits/di#readme",
  "devDependencies": {
    "app-bundler": "0.0.7",
    "atma": "^0.10.58",
    "atma-io": "^0.2.17",
    "atma-loader-babel": "^1.0.26"
  },
  "app-bundler": {
    "file": "src/Di.js",
    "outputMain": "lib/di.js",
    "package": {
      "module": "commonjs",
      "moduleWrapper": "umd",
      "moduleName": "Di",
      "commonjs": {
        "output": "simplified"
      }
    },
    "middlewares": {
      "js": [
        "importer:read",
        "atma-loader-babel:read"
      ]
    }
  }
}
